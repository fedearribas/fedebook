<div class="container">
  <div class="row search-result">
    <% @users.each do |user| %>
      <div class="col-md-8 col-md-offset-2 well member">
        <%= link_to user.full_name, user_path(user) %>
       
      </div>
    <% end %>
  </div>
  <%= will_paginate @users, renderer: BootstrapPagination::Rails %>
</div>